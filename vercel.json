{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "vite",
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 60,
      "includeFiles": "api/lib/**"
    },
    "api/**/*.py": {
      "runtime": "python3.9",
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "silent": true
  }
}